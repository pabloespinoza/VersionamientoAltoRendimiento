@{
    ViewBag.Title = "AntecedentePostulante";
}
<div class="row">
    <div class="col-md-12">
        <ol class="breadcrumb">
            <li>@Html.ActionLink("Inicio", "Index", "AntecedenteGeneralProyecto")</li>
            <li>@Html.ActionLink("Proyecto " + (string)@ViewBag.Proyecto, "Index", "AntecedenteGeneralProyecto") </li>
            <li>Antecedente Postulante</li>
        </ol>
    </div>
</div>
<div class="text-right">
    <button type="button" onclick="volver()" class="btn btn-sm btn-info pull-left" data-dismiss="modal">Volver</button>
</div>
<br />
<br />
<br />
<h4>Datos de la Organización</h4>
<br />


<form class="form-horizontal">
    <div class="form-group">
        <label class="col-sm-3 control-label">Rut Organización</label>
        <div class="col-sm-9">
            <span class="form-control" disabled="false">@ViewBag.RutOrganizacion</span>
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-3 control-label">Nombre</label>
        <div class="col-sm-9">
            <span class="form-control" disabled="false">@ViewBag.NombreOrganizacion</span>
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-3 control-label">Domicilio</label>
        <div class="col-sm-9">
            <span class="form-control" disabled="false">@ViewBag.DireccionOrganizacion</span>
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-3 control-label">Comuna</label>
        <div class="col-sm-9">
            <span class="form-control" disabled="false">@ViewBag.ComunaOrganizacion</span>
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-3 control-label">Región</label>
        <div class="col-sm-9">
            <span class="form-control" disabled="false">@ViewBag.RegionOrganizacion</span>
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-3 control-label">Email</label>
        <div class="col-sm-9">
            <span class="form-control" disabled="false">@ViewBag.EmailOrganizacion</span>
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-3 control-label">Teléfono</label>
        <div class="col-sm-9">
            <span class="form-control" disabled="false">@ViewBag.FonoOrganizacion</span>
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-3 control-label">Fax</label>
        <div class="col-sm-9">
            <span class="form-control" disabled="false">@ViewBag.FaxOrganizacion</span>
        </div>
    </div>
    <br />
    <br />
    <br />
</form>

