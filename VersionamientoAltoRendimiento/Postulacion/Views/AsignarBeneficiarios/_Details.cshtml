@model AltoRendimiento.Models.Beneficiario_Producto_Cargo
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Detalles Beneficiario</h4>
</div> 

<div class="modal-body">

    <dl class="dl-horizontal">
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Beneficiario.NombreCompleto)
        </dd>

        <dt>
            Rut
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Beneficiario.RutCompleto)
        </dd>
        <dt>
            Cargo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Cargo.carNombre)
        </dd>
    </dl>


</div>
<div class="modal-footer">
    <button class="btn btn-danger" data-dismiss="modal">Aceptar</button>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
<script>
    $('html').bind('keypress', function (e) {
        if (e.keyCode == 13) {
            return false;
        }
    });
</script>
