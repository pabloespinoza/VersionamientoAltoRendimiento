@model AltoRendimiento.Administracion.Models.ProvinciaModel

@{
    ViewBag.Title = "Detalle Provincia.";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Provincia</h4>
</div>
@using (Html.BeginForm())
{
    <div class="modal-body">
        <div class="form-horizontal">
            @Html.ValidationSummary(true)
            @Html.AntiForgeryToken()

            <div class="panel panel-default">
                <div class="panel-heading">
                    Antecedentes Provincia
                </div>
                <div class="panel-body">
                    <div class="form-group">
                        @Html.LabelFor(model => model.sRegion, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.sRegion
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.prvNombre, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.prvNombre
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
<div class="modal-footer">
    <button type="button" class="btn btn-sm btn-info pull-left" data-dismiss="modal">Volver</button>
</div>
