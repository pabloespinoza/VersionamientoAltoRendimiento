@model AltoRendimiento.Administracion.Models.ComunaModel

@{
    ViewBag.Title = "Detalle Comuna.";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Comuna</h4>
</div>
@using (Html.BeginForm())
{
    <div class="modal-body">
        <div class="form-horizontal">
            @Html.ValidationSummary(true)
            @Html.AntiForgeryToken()

            <div class="panel panel-default">
                <div class="panel-heading">
                    Antecedentes Comuna
                </div>
                <div class="panel-body">
                    <div class="form-group">
                        @Html.LabelFor(model => model.sProvincia, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.sProvincia
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.comNumeroComuna, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.comNumeroComuna
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.comNombre, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.comNombre
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
<div class="modal-footer">
    <button type="button" class="btn btn-sm btn-info pull-left" data-dismiss="modal">Volver</button>
</div>
