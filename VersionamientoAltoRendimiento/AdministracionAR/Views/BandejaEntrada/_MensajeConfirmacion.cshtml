@model AltoRendimiento.Administracion.Models.ProyectoModel

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Comfirmacion de Cambio</h4>
</div>

<div class="modal-body">

    <div class="form-group">

        <fieldset disabled>
            <div class="form-group">
                <label for="lblEstado" class="control-label col-md-4">Codigo Proyecto </label>
                <div class="col-md-7">
                    <label class="form-control col-md-4">@Model.IdProyecto</label>      
                </div>
            </div>
            <div class="form-group">
                <label for="lblFecha" class="control-label col-md-4">Fecha</label>
                <div class="col-md-7">
                    <label class="form-control col-md-4">@Model.FechaEvaluacion</label> 
                </div>
            </div>

            <div class="form-group">
                <label for="lblUsuario" class="control-label col-md-4">Usuario</label>
                <div class="col-md-7">
                    <label class="form-control col-md-4">@Model.UsuarioCambio</label>      
                </div>
            </div>
        </fieldset>
    </div>

    <div>
        <div class="form-group">
            <fieldset disabled>
                <legend>Información de la acción.</legend>
                @Model.Mensaje
            </fieldset>
        </div>


    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-sm btn-danger pull-left" data-dismiss="modal">Cancelar</button>
    <button type="submit" class="btn btn-sm btn-success " data-dismiss="modal" aria-hidden="true">Aceptar</button>
</div>
