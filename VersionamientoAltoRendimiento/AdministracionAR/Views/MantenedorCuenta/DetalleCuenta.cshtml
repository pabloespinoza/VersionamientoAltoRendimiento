@model AltoRendimiento.Administracion.Models.CuentaModel

@{
    ViewBag.Title = "Detalle Cuenta.";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Cuenta</h4>
</div>
@using (Html.BeginForm())
{
    <div class="modal-body">
        <div class="form-horizontal">
            @Html.ValidationSummary(true)
            @Html.AntiForgeryToken()

            <div class="panel panel-default">
                <div class="panel-heading">
                    Antecedentes Cuenta
                </div>
                <div class="panel-body">
                    <div class="form-group">
                        @Html.LabelFor(model => model.sCentroFinanciero, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.sCentroFinanciero
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.ctaAno, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.ctaAno
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.ctaNumeroCuenta, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.ctaNumeroCuenta
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.ctaNombreCuenta, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.ctaNombreCuenta
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.ctaPresupuestoVigente, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.ctaPresupuestoVigente
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.ctaReintegro, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.ctaReintegro
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.ctaGirado, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.ctaGirado
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.ctaAsignado, new { @class = "col-md-4" })
                        <div class="col-md-8">
                            @Model.ctaAsignado
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
<div class="modal-footer">
    <button type="button" class="btn btn-sm btn-info pull-left" data-dismiss="modal">Volver</button>
</div>
