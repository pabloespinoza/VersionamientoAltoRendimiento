@model AltoRendimiento.Administracion.Models.TipoGastoPersonalModel

@{
    ViewBag.Title = "Detalle Tipo Gasto Personal.";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Tipo Gasto Personal</h4>
</div>
@using (Html.BeginForm())
{
    <div class="modal-body">
        <div class="form-horizontal">
            @Html.ValidationSummary(true)
            @Html.AntiForgeryToken()

            <div class="panel panel-default">
                <div class="panel-heading">
                    Antecedentes Tipo Gasto Personal
                </div>
                <div class="panel-body">
                    <div class="form-group">
                        @Html.LabelFor(model => model.tgpNombre, new { @class = "col-md-6" })
                        <div class="col-md-6">
                            @Model.tgpNombre
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.TipoGastoConverge, new { @class = "col-md-6" })
                        <div class="col-md-6">
                            @Model.DescripcionGastoConverge
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.tgpDescripcion, new { @class = "col-md-6" })
                        <div class="col-md-6">
                            @Model.tgpDescripcion
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.tgpNumeroGastoPersonal, new { @class = "col-md-6" })
                        <div class="col-md-6">
                            @Model.tgpNumeroGastoPersonal
                        </div>
                    </div>
                 </div>
            </div>
        </div>
    </div>
}
<div class="modal-footer">
    <button type="button" class="btn btn-sm btn-info pull-left" data-dismiss="modal">Volver</button>
</div>
