    <script type="text/javascript">

        $(document).ready(function () {
            $("#Administracion").addClass("hide");
            $('#Asignacion').addClass("hide");
            $('#Financimiento').addClass("hide");
            $('#Configuracion').addClass("hide");

            var adm = '@AltoRendimiento.Administracion.Util.SessionIND.Current.UsuarioModel.IdRol'   
            var rol = '@AltoRendimiento.Administracion.Util.SessionIND.Current.UsuarioModel.IdRol'
            if (adm == 13) {
                $('#Configuracion').addClass("show");
                $('#Financimiento').addClass("show");
                $('#Asignacion').addClass("show");
                $("#Administracion").addClass("show");
            }

            if (adm == 9) {
                $('#Financimiento').addClass("show");
                $('#Asignacion').addClass("show");
                $("#Administracion").addClass("show");
            }

            if (adm == 3) {
                $('#Asignacion').addClass("show");
            }

        });

    </script>
